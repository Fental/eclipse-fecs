<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.eclipsesource.jshint.feature"
      label="JSHint Eclipse Integration"
      version="0.9.11.qualifier"
      provider-name="EclipseSource">

   <description url="http://github.eclipsesource.com/jshint-eclipse/">
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update url="http://github.eclipsesource.com/jshint-eclipse/updates/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.ui" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime" version="3.6.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="com.eclipsesource.jshint"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.eclipsesource.jshint.source"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.eclipsesource.jshint.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.eclipsesource.jshint.ui.source"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.mozilla.javascript"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.mozilla.javascript.source"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
